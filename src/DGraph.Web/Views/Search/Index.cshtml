@model dynamic

@{
    ViewBag.Title = "Search Dependencies";
}

<h2>@ViewBag.Title</h2>
<div class="row">
    <form class="col-md-12" role="search" id="search-form">
        <div class="form-group col-md-12">
            <label for="Keywords">Keywords: </label>
            <input type="text" class="form-control" placeholder="Search keywords" name="Keywords" id="Keywords" />
        </div>
        <div class="form-group col-md-12">
            <label for="Name">Name: </label>
            <input type="text" class="form-control" placeholder="Search by name" name="Name" id="Name" />
        </div>
        <br />
        <br />
        <div class="form-group col-md-12">
            <button type="button" class="btn btn-default form-control" id="submit">Submit</button>
        </div>
    </form>
</div>

<div class="row col-lg-12 col-md-12">
    <div class="col-md-2 col-lg-2">ApplicationName</div>
    <div class="col-md-2 col-lg-2">Id</div>
    <div class="col-md-2 col-lg-2">TargetFramework</div>
    <div class="col-md-2 col-lg-2">Type</div>
    <div class="col-md-2 col-lg-2">Version</div>
    <div class="col-md-2 col-lg-2">DateTime</div>
</div>
<div class="row" id="results">
    <div class="row col-lg-12 col-md-12">
        <h2>No results</h2>
    </div>
</div>
@section scripts{
    <script src="@Url.Content("~/bundles/search")" type="text/javascript"></script>
}
